<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title> #(title) </title>

    <!--fontawesome-->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css" integrity="sha384-5sAR7xN1Nv6T6+dT2mhtzEpVJvfS3NScPQTrOxhwjIuvcA67KV2R5Jz6kr4abQsz" crossorigin="anonymous">

    <link rel="stylesheet" href="./styles/css/master.css">

    <!-- nav bar -->
    <link rel="stylesheet" href="./styles/css/circle-nav/normalize.css">
    <link rel="stylesheet" href="./styles/css/circle-nav/nav-design.css">
    <link rel="stylesheet" href="./styles/css/preloader/global-preloader.css">
    <script src="./styles/js/modernizr-2.6.2.min.js"></script>

    <!-- jquery -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha256-3edrmyuQ0w65f8gfBsqowzjJe2iM6n0nKciPUp8y+7E=" crossorigin="anonymous"></script>

  </head>
  <body class="radial-blur-background">

    <!-- Preloader -->
    <div id="preloader">
        <div class="spinner"></div>
    </div>
    <!-- circle navigation bar -->
    #if(title != "Log In"){
      <button class="cn-button" id="cn-button">+</button>
      <div class="cn-wrapper" id="cn-wrapper">
        <ul>
           <li #if(title == "User"){class="active"}><a href="/user"><span class="fas fa-cogs"></span></a></li>
           <li><a href="#"><span class="fas fa-user-circle"></span></a></li>
           <li #if(title == "Home"){class="active"}><a href="/"><span class="fas fa-home"></span></a></li>
           <li><a href="#"><span class="fas fa-user-friends"></span></a></li>
           <li><a href="#"><span class="fas fa-times"></span></a></li>
         </ul>
        </div>
      <div id="cn-overlay" class="cn-overlay"></div>
    }

    <!-- nav bar -->
    <script type="text/javascript" src="./styles/js/polyfills.js"></script>
    <script type="text/javascript" src="./styles/js/nav.js"></script>
    <script type="text/javascript" src="./styles/js/global-preloader.js"></script>
    #get(content)
  </body>
</html>
